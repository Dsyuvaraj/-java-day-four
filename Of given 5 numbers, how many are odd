public class Main {
    public static void main(String[] args) {
        int[] numbers = { 3, 8, 12, 5, 6 };
        int oddCount = 0;

        for (int number : numbers) {
            if (number % 2 != 0) {
                oddCount++;
            }
        }

        System.out.println("The number of odd numbers is: " + oddCount);
    }
}
