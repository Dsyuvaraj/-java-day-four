public class Main {
    public static void main(String[] args) {
        int[] numbers = { 3, 8, 12, 5, 6 };
        int evenCount = 0;

        for (int number : numbers) {
            if (number % 2 == 0) {
                evenCount++;
            }
        }

        System.out.println("The number of even numbers is: " + evenCount);
    }
}
